
# coding: utf-8

# In[ ]:

for p in range(1,101):
    print p
    x = input()
    x = int(x)
    if x == 2 :
        print (1)
    else :
        for j in range(2, x) :
            if x % j == 0 :
                print (0)
                break
            elif j == x - 1 :
                print (1)
                break


