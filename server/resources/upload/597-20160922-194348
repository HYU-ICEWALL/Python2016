

def isDivisor(num) :
	i=0;
	divisor = 0;
	for i in xrange(1,num+1):
		if(num%i == 0):
			divisor += 1;
	return(divisor == 2)



for i in xrange(100) :
	print "input a number : ",
	a = raw_input()
	if(isDivisor(int(a))) :
		print a + " is a decimal fraction"
		continue
	print a + " is not a decimal fraction"