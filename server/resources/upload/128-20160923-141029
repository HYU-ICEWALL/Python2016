print("#example of word2vec including GoogleNews-pre-binary-data")
from gensim.models import word2vec
import gensim
print("\n#load word2vec bin")
print("model = word2vec.Word2Vec.load_word2vec_format('GoogleNews-vectors-negative300.bin', binary=True)")
model = word2vec.Word2Vec.load_word2vec_format('data/GoogleNews-vectors-negative300.bin', binary=True)
while True:
    print("#input keyword: ", end="");
    command = input()
    if command == 'q':
        break
    result = model.most_similar([command])
    print ("result: " + str(result))
