dic = {}
lol = int(input())
for i in range(lol):
    s = input().split()
    dic[s[0]] = int(s[1])
lol = int(input())
for i in range(lol):
    s = input().split()
    if(s[0] == "a"):
        dic[s[1]] += int(s[2])
    elif(s[0] == "m"):
        dic[s[1]] -= int(s[2])
    elif(s[0] == "s"):
        print(dic[s[1]])
