
# coding: utf-8

# In[2]:

user_input = []
for i in range(100):
    user_input.append(int(input()))

for index in user_input:
    cnt = 0
    for loop in range(2, index+1):
        if(index%loop == 0):
            cnt+=1
        if(cnt > 1):
            print (0)
            break
        if(loop == index):
            print (1)


